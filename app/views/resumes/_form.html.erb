<%= simple_form_for @resume do |f| %>
  <label>Name:</label>
  <%= f.text_field :name %>
  <label>Location:</label>
  <%= f.text_field :location %>
  <label>Phone Number:</label>
  <%= f.text_field :phone_number %>
  <label>Biography:</label>
  <%= f.text_area :biography %>
  <br/>
  <%= f.submit %>
<% end %>
