<h1>Edit this task</h1>
<%= simple_form_for([@project, @task]) do |f|  %>

  <%= f.input :title %>

  <%= f.input :description %>

  <%= f.select :job_type, options_for_select(["Programming", "Music", "Art", "Story", "Level Design", "Puzzles", "Miscellaneous"], @task.job_type)  %>
  <%= f.submit %>
<% end %>
<%= link_to "Cancel", @project %>
